<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="uit.tkorg.cspublicationtool.model.Magazine" table="magazine"/>
  
        <id column="idMagazine" name="idMagazine" type="integer">
            <generator class="native"/>
        </id>
        <property column="magazineName" name="magazineName" type="string"/>
        <property column="website" name="website" type="string"/>
        <property column="organzation" name="organzation" type="string"/>
        <property column="yearStart" name="yearStart" type="integer"/>
        <property column="yearEnd" name="yearEnd" type="integer"/>
        <property column="url" name="url" type="string"/>  
         
</hibernate-mapping>
