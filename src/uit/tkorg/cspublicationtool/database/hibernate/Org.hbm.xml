<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="uit.tkorg.cspublicationtool.model.Org" table="org"/>
        <id column="idOrg" name="idOrg" type="integer">
            <generator class="native"/>
        </id>
        <property column="orgrName" name="orgrName" type="string"/>
        <property column="continent" name="continent" type="string"/>
        <property column="website" name="website" type="string"/>
        <property column="h_index;" name="h_index" type="integer"/>
        <property column="url" name="url" type="string"/>
        <many-to-one name="authorInstance" class="AuthorInstance" column="authorinstance"/>
</hibernate-mapping>
