<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="uit.tkorg.cspublicationtool.model.Paper" table="paper"/>
  
         <id column="idPaper" name="idPaper" type="integer">
            <generator class="native"/>
        </id>
        <property column="doi" name="doi" type="string"/>
        <property column="isbn" name="isbn" type="string"/>
        <property column="isbn" name="isbn" type="string"/>
        <property column="url" name="url" type="string"/>
        <property column="title" name="title" type="string"/>
        <property column="abstractPaper" name="abstract" type="string"/>
        <property column="viewPublicaiton" name="viewPublicaiton" type="string"/>
        <property column="bibTex" name="bibTex" type="string"/>
        <property column="year" name="year" type="integer"/>
         <property column="endNote" name="endNote" type="string"/>
        <property column="dblpKey" name="dblpKey" type="string"/>
        <property column="vesion" name="vesion" type="string"/>
        <property column="paperFile" name="paperFile" type="string"/>
        <many-to-one name="author" class="Author" column="author"/>
  
         <set name="papers" table="author_paper">
            <key column="idAuthor"/>
            <many-to-many column="paper" class="Paper"/>
           </set>

</hibernate-mapping>
